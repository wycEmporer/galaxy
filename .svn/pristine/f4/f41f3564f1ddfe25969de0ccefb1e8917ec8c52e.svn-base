import Vue from 'vue';
import Router from 'vue-router';
import Trip from './views/Trip';
import TripDetail from './views/TripDetail';

Vue.use(Router);
export default new Router({
  mode: 'history',
  routes: [
    {
      path: '/trip',
      name: 'trip',
      component: Trip
    },
    {
      path: '/trip/detail',
      name: 'tripdetail',
      component: TripDetail
    }
  ]
});
